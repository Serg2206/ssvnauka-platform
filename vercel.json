{
  "buildCommand": "npm run build",
  "devCommand": "npm dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "env
    "DATABASE_URL": "database_url",
    "NEXTAUTH_SECRET": "nextauth_secret",
    "NEXTAUTH_URL": "nextauth_url"
  },
  "build": {    "env": {
      "NEXT_PUBLIC_APP_UL": "https://ssvnauka.com"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    }
  ]
}
